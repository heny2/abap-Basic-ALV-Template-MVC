*&      Module  MOD_CLEAR_FCODE  INPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE mod_clear_fcode INPUT.
  CLEAR:sy-ucomm.
ENDMODULE.
*&---------------------------------------------------------------------*
*&      Module  MOD_EXIT_COMMAND_9000  INPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE mod_exit_command_9000 INPUT.
  ok-code = sy-ucomm.
  CASE ok-code.
    WHEN 'BACK' OR 'LEAV' OR 'CANC'.
      LEAVE TO SCREEN 0.
  ENDCASE.
ENDMODULE.
